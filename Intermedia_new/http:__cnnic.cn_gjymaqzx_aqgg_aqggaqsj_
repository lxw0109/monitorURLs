    <script language="javascript">
        function chanageMenu(id, title, content) {
            for (var i = 1; i <= 3; i++) {
                $("#" + content + i).hide();
                $("#" + title + i + " a").removeClass("hover");
            }
            $("#" + content + id).show();
            $("#" + title + id + " a").addClass("hover");
        }
        function menuHover(index, title, content) {
            //            for (var i = 1; i <= 12; i++) {
            //                $("#" + content + i).hide();
            //                $("#" + title + i).removeClass("on");
            //            }
            //            $("#" + content + id).show();
            //            $("#" + title + id).addClass("on");
            $(".navitem").each(function (i) {
                var id = i + 1;
                if (id == index) {
                    $(this).removeClass("on");
                    $(this).addClass("on");
                    $("#" + content + id).show();
                }
                else {
                    $(this).removeClass("on");
                    $("#" + content + id).hide();
                }
            });
        }

        function showMenu(index) {
            if ($("#child" + index).css("display") != "none") {
                //                $("#child" + index).css("display", "none");
                $("#child" + index).slideUp("slow");

            }
            else {
                //                $("#child" + index).css("display", "inline");
                $("#child" + index).slideDown("slow");
            }
        }

        $(document).ready(function () {
            $(".item").each(function () {
                $(this).mouseover(function () {
                    $(this).addClass("hover");
                });
                $(this).mouseout(function () {
                    $(this).removeClass("hover");
                });
            });
        });
    </script>
