<script src="themes/cira/javascript/cira.js"></script>
<script type="text/javascript">
    // <![CDATA[
    $.noConflict();

    jQuery(document).ready(function($){
        //remove class 'text' from inputs we're attaching virtual keyboard to
        jQuery('#domain, #domain_footer, #avail-search-box').removeClass('text');

        jQuery('.text').defaultValue();

        //placeholder replacement
        if (!Modernizr.input.placeholder) {

            jQuery('#domain, #domain_footer').each(function () {
                if (!jQuery(this).val()) {
                    this.value = jQuery(this).attr('placeholder');
                }
                jQuery(this).focus(function () {
                    if (this.value == jQuery(this).attr('placeholder')) {
                        this.value = '';
                    }
                });
                jQuery(this).blur(function () {
                    //if input's value is blank, and currently focused item is not a child of .virtual-keyboard-wrapper
                    //reset input value to original placeholder text
                    if (this.value == '' && jQuery(':focus').parents('.virtual-keyboard-wrapper').length <= 0) {
                        this.value = jQuery(this).attr('placeholder');
                    }

                    //reset the focus to this if the user clicked a virtual keyboard key
                    if(jQuery(':focus').parents('.virtual-keyboard-wrapper').length){
                        jQuery(this).focus();
                    }
                });
            });

        }
    });
    // ]]>
</script>


