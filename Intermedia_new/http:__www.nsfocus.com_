         } else if (lang.indexOf('ja') == 0) {
                   document.location.href = 'http://jp.nsfocus.com/index.html'
         }
} else {
         var date=new Date();
         date.setTime(date.getTime()+20*24*3600*1000);
         document.cookie = 'nsfocusLanguage=en;domain=;expire="'+date+'+;path=/'; 
}
})();
</script>

