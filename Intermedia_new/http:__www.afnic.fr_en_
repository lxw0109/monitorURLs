    <script type="text/javascript">
        var name = "AFNIC_COOKIE_POLICY";
        var cookiePolicyBlock = document.getElementById("cookie-policy");
        var policyValue = $.cookie(name);
        var body = document.getElementsByTagName("body")[0];

        function enableCookieBlock() {
            body.insertBefore(cookiePolicyBlock, body.firstChild);
            cookiePolicyBlock.style.display = "inherit";
            body.style.backgroundPosition = "left 72px";
        }

        function disableCookieBlock() {
            cookiePolicyBlock.style.display = "none";
            body.style.backgroundPosition = "left 0";
        }

        function setCookiePolicy(value) {
            var d = new Date();
            d.setTime(d.getTime() + 31536000000);
            $.cookie(name, value, { expires: d, path: "/" });
        }

        (function() {
            if (policyValue !== null) {
                disableCookieBlock();
            } else {
                enableCookieBlock();
            }

            var disableLink = document.getElementById("disable-cookie-policy");
            disableLink.onclick = function() {
                setCookiePolicy(1);
                disableCookieBlock();
            };

            var declineLink = document.getElementById("decline-cookies");
            declineLink.onclick = function() {
                setCookiePolicy(0);
                disableCookieBlock();
            }
        })();
    </script>
