		<!--// OUTBOUND LINKS EVENT //-->
		<script type="text/javascript">

			window.onload=function(){

			//Find all links that have target="_blank" and add attribute of onclick
			var fullTarget = jQuery("a[target='_blank']");

			for (var i = 0; i < fullTarget.length; i++)
			{
				//Define the onclick function
				var onclick_fxn = "_gaq.push(['_trackEvent', 'Outbound Links', '"+ jQuery(fullTarget[i]).attr('href') +"']);";
				//var onclick_fxn = "trackOutboundLink(this, 'Outbound Links', '"+ jQuery(fullTarget[i]).attr('href') +"'); return false;";
				//Add the attribute
				jQuery(fullTarget[i]).removeAttr('target');
				jQuery(fullTarget[i]).attr('onclick', onclick_fxn);
				jQuery(fullTarget[i]).attr('target','_blank');
			}
		};

		//Split the Cookies
		function ReadCookie(name) 
			{
			  name += '=';
			  var parts = document.cookie.split(/;\s*/);
			  for (var i = 0; i < parts.length; i++)
			  {
			    var part = parts[i];
			    if (part.indexOf(name) == 0)
			      return part.substring(name.length)
			  }
			  return null;
			}

		//Outbound Link Tracking

			function trackOutboundLink(link, category, action) { 
	 
				try { 
				_gaq.push(['_trackEvent', category , action]); 
			} catch(err){}
		 
			setTimeout(function() {
				document.location.href = link.href;
			}, 100);
		}

		</script>
	
