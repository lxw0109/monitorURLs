<script>
    (function(){
        // Set a random glyph

        // Random colour by default, but some sections are themed a particular colour
        var colour = _.sample(["blue" , "darkgreen" , "lightgreen" , "orange" , "purple"]);
        var path = window.location.pathname;
        if(path.match(/^\/services/)){colour = "orange"}
        else if(path.match(/^\/community/)){colour = "darkgreen"}
        else if(path.match(/^\/events/)){colour = "purple"}
        else if(path.match(/^\/about/)){colour = "blue"}
        else if(path.match(/^\/publications/)){colour = "lightgreen"}
        var position = _.random(1, 5);
        var selector = "apnic-sprite apnic-sprite-glyphs glyph-" + colour + "-" + position + " hidden-print";
        $('#glyph').attr('class',selector);

        // Add your IP address
        $.ajax({
            url: "https://cgi1.apnic.net/cgi-bin/my-ip.php",
            jsonp: "callback",
            dataType: "jsonp",
            success: function( response ) {
                var $node = $('#ip');
                var txt = $node.text();
                var ip = response.ip;
                var ipv6 = ip.indexOf(':')!=-1;
                var url = "https://www.apnic.net/apnic-info/whois_search/your-ip";
                var markup = txt + " <a href='" + url + "' data-ga-category='Banner' data-ga-event='click' data-ga-label='Your IP address' class='ga-event'>" + ip + "</a>";
                $node.html(markup);
                if(ipv6){
                    $node.prepend('<strong id="via-ipv6" class="apnic-sprite apnic-sprite-icons"><span>Connected Via IPv6</span></strong>');
                }
            }
        });
    }).call(this);
</script>
						
