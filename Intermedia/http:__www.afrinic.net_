<script type="text/javascript">

      
	
	  
	jQuery.noConflict();   
	jQuery(document).ready(function($) {
   // put all your jQuery goodness in here.
   // Do not instantiate elements until the document has finished loading

	//Accordion functions
	$('.accordion-list:not(:first)').hide();	
	$('.accordion-list:first').show();	
	$('.accordion-header:first').addClass('active');	
	$('.accordion-list:first').addClass('content-active');
	
	$('.accordion-header').click(function () {
		$('.accordion-list:visible').slideUp().prev().removeClass('active');
		$('.accordion-list:visible').removeClass('content-active');
		$(this).addClass('active').next().slideDown();
		$('.accordion-list').addClass('content-active');
		
	});
	
	//$("#advancedQueryRow").hide();
	$('#adv_whois_opt').click(function () {
	    
	  if(this.checked == true){   
	   $("#queryType").attr("value","advanced");     
	   //$("#advancedQueryRow").show();
	   $("#advancedQueryRow").css("display","block");
	  }else{
	   $("#queryType").attr("value","simple");
	   //$("#advancedQueryRow").hide();
	   $("#advancedQueryRow").css("display","none");
	  }
	  
	 });
	
	 $('#btnReset').click(function () {
	  $('#query').attr("value","");
	  
	  $('select[name=alt_database]').val('AFRINIC');
	  $('select[name=object_type]').val('All');
	  $('select[name=ip_search_lvl]').val('Default');
	  $('select[name=inverse_attributes]').val('None'); 
	  
	  $('input[class=chkBox]:checked').removeAttr('checked');   
	 });
 
});
	  
