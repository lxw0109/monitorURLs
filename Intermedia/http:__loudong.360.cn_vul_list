<script>
function to_Searchcheck(){
	var ld_content = $("#ld_content").val();
	ld_content = ld_content.replace(/(^\s*)|(\s*$)/g, ""); 
	if(ld_content.length < 1){
		$("#search_error").html("请输入查询内容！");
		return false;
	}

	if(str_len(ld_content) < 2){
		$("#search_error").html("查询关键字长度至少是2位！");
		return false;
	}
	if(str_len(ld_content) > 20){
		$("#search_error").html("搜索关键字太长！");
		return false;
	}
	if(ld_content == "QTVA" || ld_content == "QTVA-2013" || ld_content == "2013"){
		$("#search_error").html("请输入有效的搜索关键字！");
		return false;
	}
	//ld_content = ld_content.replace(".php", "");
	window.location.href="/vul/search/tag/"+encodeURIComponent(ld_content)+"/";
	return false;
}
$("#ld_content").focus(function(){
	$("#search_error").html("");	
});
$("#ld_content").click(function(){
	$("#search_error").html("");	
});
$("#query_btn").click(function(){
	$("#search_form").submit();
});
function str_len(s) {
	var l = 0;
	var a = s.split("");
	for (var i=0;i<a.length;i++) {
	 if (a[i].charCodeAt(0)<299) {
	  l++;
	 } else {
	  l+=2;
	 }
	}
	return l;
}

