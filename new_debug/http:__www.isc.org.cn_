<script>
function trim_str(str)
{       
    var i,j; 
    if(str == "")
        return ""; 
    for(i=0;i<str.length;i++) 
        if(str.charAt(i) != ' ') break; 
    if(i >= str.length) return "";
    for(j=str.length-1;j>=0;j--) 
    if(str.charAt(j) != ' ') break;
    return str.substring(i,j+1); 
} 

